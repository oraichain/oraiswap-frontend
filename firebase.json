{
    "hosting": [
        {
            "target": "oraiswap",
            "public": "build",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ]
        },
        {
            "target": "oraiswap-staging",
            "public": "build",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "headers": [
                {
                    "source": "/service-worker.js",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "no-cache"
                        }
                    ]
                }
            ],
            "rewrites": [
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ]
        },
        {
            "target": "oraiswap-testnet",
